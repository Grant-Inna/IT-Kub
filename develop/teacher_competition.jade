            .wrapper
               h1.title.mb4 Задачи конкурса
               .row
                  include assets/jade/content/teacher_competition/__task
                  +task( '01', 'Совершенствование профессионального мастерства педагогов дополнительного образования детей' )
                  +task( '02', 'Повышение общественного и&nbsp;профессионального статуса педагогов дополнительного образования детей' )

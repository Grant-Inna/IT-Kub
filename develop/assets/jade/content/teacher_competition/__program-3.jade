mixin program-3( t1, t2, caption, title, name, topic, name2, topic2, name3, topic3, name4, topic4, place )
   .program_row
      .program_row__time #{t1}&nbsp;&ndash; #{t2}
      .meeting_program__info
         .hackathon_program__caption.mb2.uppercase !{caption}
         .program_row_topic.mb15 !{title}
         .flex
            .t-competition_icon_dot-1
            .mr1.ml1 1
            .block
               .program_row_person.mb08 !{name}
                  i.ml03 !{topic}

         .flex
            .t-competition_icon_dot-2
            .mr1.ml1 2
            .block
               .program_row_person.mb08 !{name2}
                  i.ml03 !{topic2}

         .flex
            .t-competition_icon_dot-3
            .mr1.ml1 3
            .block
               .program_row_person.mb08 !{name3}
                  i.ml03 !{topic3}

         .flex
            .t-competition_icon_dot-4
            .mr1.ml1 4
            .block
               .program_row_person !{name4}
                  i.ml03 !{topic4}


      .program_row_place.top !{place}

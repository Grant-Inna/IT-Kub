mixin program-4( t1, t2, title, name, topic, name2, topic2, name3, topic3, name4, topic4, place )
   .program_row
      .hackathon_program__time #{t1}&nbsp;&ndash; #{t2}
      .meeting_program__info
         .hackathon_program__caption !{title}
         .meeting_program__person !{name}
         .meeting_program__topic.mb05 !{topic}
         .meeting_program__person !{name2}
         .meeting_program__topic.mb05 !{topic2}
         .meeting_program__person !{name3}
         .meeting_program__topic.mb05 !{topic3}
         .meeting_program__person !{name4}
         .meeting_program__topic !{topic4}
      .hackathon_row_place !{place}
